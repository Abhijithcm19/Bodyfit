<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Login V8</title>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!--===============================================================================================-->
    <link
      rel="icon"
      type="image/png"
      href="/public/forgot/images/icons/favicon.ico"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="/public/forgot/vendor/bootstrap/css/bootstrap.min.css"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="/public/forgot/fonts/font-awesome-4.7.0/css/font-awesome.min.css"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="/public/forgot/vendor/animate/animate.css"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="/public/forgot/vendor/css-hamburgers/hamburgers.min.css"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="/public/forgot/vendor/animsition/css/animsition.min.css"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="/public/forgot/vendor/select2/select2.min.css"
    />
    <!--===============================================================================================-->
    <link
      rel="stylesheet"
      type="text/css"
      href="/public/forgot/vendor/daterangepicker/daterangepicker.css"
    />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="/public/forgot/css/util.css" />
    <link rel="stylesheet" type="text/css" href="/public/forgot/css/main.css" />
    <!--===============================================================================================-->

    <!--===============================================================================================-->
  </head>
  <body>
    <div class="limiter">
      <div class="container-login100">
        <div class="wrap-login100">
          <form
            class="login100-form validate-form p-l-55 p-r-55 p-t-178"
            action="/admin/stafftable/add-staff"
            method="post"
          >
            <span class="login100-form-title"> New Staff </span>
            <div
              class="wrap-input100 validate-input"
              data-validate="Please enter password"
            >
              <input
                class="input100"
                type="text"
                name="name"
                placeholder="User Name"
              />
              <span class="focus-input100"></span>
            </div>
            <br />
            <div
              class="wrap-input100 validate-input"
              data-validate="Please enter password"
            >
              <input
                class="input100"
                type="email"
                name="email"
                placeholder="Email"
              />
              <span class="focus-input100"></span>
            </div>
            <br />
            <div
              class="wrap-input100 validate-input"
              data-validate="Please enter password"
            >
              <input
                class="input100"
                type="password"
                name="password"
                placeholder="Password"
              />
              <span class="focus-input100"></span>
            </div>

            <div class="text-right p-t-13 p-b-23"></div>

            <div class="container-login100-form-btn">
              <button class="login100-form-btn">Submit</button>
            </div>

            <div class="flex-col-c p-t-170 p-b-40"></div>
          </form>
        </div>
      </div>
    </div>
  </body>
</html>
