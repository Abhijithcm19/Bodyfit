        addToWishlistForm.addEventListener('submit', event => {
          event.preventDefault();
      
          const productId = addToWishlistForm.productId.value;
          fetch('/viewWishlist', {
            method: 'post',
            body: JSON.stringify({ productId: productId }),
            headers: { 'Content-Type': 'application/json' }
          })
            .then(response => response.json())
            .then(data => {
              alert(data.message);
            })
            .catch(error => {
              console.error(error);
              alert('Error adding product to wishlist');
            });
        });
